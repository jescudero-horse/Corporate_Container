<div class="mb-3">
    <div class="mb-3 form-floating">
        <input type="url" class="form-control" id="location" name="location" placeholder="Location/URL">
        <label for="location" class="form-label" style="color: #6c757d;">Location/URL</label>
    </div>
</div>

<div class="mb-3">
    <div class="mb-3 form-floating">
        <input type="text" class="form-control" id="description" name="description" placeholder="Description">
        <label for="location" class="form-label" style="color: #6c757d;">Description</label>
    </div>
</div>

<div class="mb-3">
    <div class="mb-3 form-floating">
        <input type="url" class="form-control" id="videoTutorial" name="videoTutorial" placeholder="Video tutorial">
        <label for="videoTutorial" class="form-label" style="color: #6c757d;">Video tutorial</label>
    </div>
</div>

<div class="mb-3">
    <div class="mb-3 form-floating">
        <input type="text" class="form-control" id="dataSource" name="dataSource" placeholder="Data source">
        <label for="dataSource" class="form-label" style="color: #6c757d;">Data source</label>
    </div>
</div>

<div class="form-check form-switch mb-3">
    <input class="form-check-input" type="checkbox" role="switch" id="sentElementals" name="sentElementals">
    <label class="form-check-label" for="sentElementals">Sent elementals</label>
</div>

<div class="form-check form-switch mb-3">
    <input class="form-check-input" type="checkbox" role="switch" id="renault" name="renault">
    <label class="form-check-label" for="renault">Renault</label>
</div>

<div class="input-group mb-3">
    <div class="mb-3 form-floating">
        <input type="contact" class="form-control" id="contact" name="contact" placeholder="Reference email">
        <label for="contact" class="form-label" style="color: #6c757d;">Contact</label>
    </div>
</div>

<div class="mb-3">
    <div class="mb-3 form-floating">
        <input type="comment" class="form-control" id="comments" name="comments" placeholder="Reference email">
        <label for="comment" class="form-label" style="color: #6c757d;">Comments</label>
    </div>
</div>

<input type="hidden" id="id" name="id">